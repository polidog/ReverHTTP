{
  "name": "reverhttp",
  "displayName": "ReverHTTP",
  "description": "Language support for ReverHTTP DSL (.rever files)",
  "version": "0.1.0",
  "publisher": "polidog",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:rever"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "rever",
        "aliases": [
          "ReverHTTP",
          "rever"
        ],
        "extensions": [
          ".rever"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "typescript": "^5.3.0"
  }
}
